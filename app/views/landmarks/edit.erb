<form action="/landmarks/<%= @landmark.id %>" method="post">
  <input type="hidden" id="hidden" name="_method" value="patch">
  <!-- landmark name input -->
  <label for="landmark_name">Landmark Name: </label><input type="text" name="landmark[name]" id="name" value="<%= @landmark.name %>"> <br><br>
  <label for="landmark_year_completed">Landmark Year Completed: </label><input type="text" name="landmark[year_completed]" id="year_completed" value="<%= @landmark.year_completed %>"> <br><br>
  <!-- submit -->
  <input type="submit" value="Edit Landmark">
</form>
